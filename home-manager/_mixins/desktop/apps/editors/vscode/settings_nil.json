{
  "[nix]": {
    "editor.defaultFormatter": "jnoortheen.nix-ide"
  },
  "direnv.restart.automatic": true,
  "direnv.status.showChangesCount": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always"
  },
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorSurroundingLines": 1.2,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.cursorWidth": 1.6,
  "editor.codeLens": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "editor.find.autoFindInSelection": "multiline",
  "editor.fontFamily": "'Liga SFMono Nerd Font Italic', 'FiraCode Nerd Font', 'JetbrainsMono Nerd Font SemiBold Italic', 'SauceCodePro Nerd Font Propo Regular Italic', 'Droid Sans Mono'",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.fontVariations": true,
  "editor.fontWeight": 500,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.indentation": true,
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": true,
  "editor.lineHeight": 1.6,
  "editor.lineNumbers": "on",
  "editor.minimap.autohide": true,
  "editor.minimap.maxColumn": 100,
  "editor.renderControlCharacters": true,
  "editor.quickSuggestions": {
    "comments": "off",
    "other": "on",
    "strings": "off"
  },
  "editor.quickSuggestionsDelay": 1,
  "editor.renderWhitespace": "trailing",
  "editor.roundedSelection": true,
  "editor.rulers": [
    100,
    140
  ],
  "editor.scrollbar.horizontal": "auto",
  "editor.scrollbar.vertical": "auto",
  "search.followSymlinks": false,
  "search.searchEditor.doubleClickBehaviour": "goToLocation",
  "search.smartCase": true,
  "search.sortOrder": "default",
  "editor.smoothScrolling": true,
  "editor.stickyScroll.defaultModel": "indentationModel",
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.maxLineCount": 3,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.stickyTabStops": true,
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 140,
  "editor.wrappingIndent": "indent",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrderLexicographicOptions": "default",
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.autoGuessEncoding": true,
  "files.autoSave": "afterDelay",
  "files.enableTrash": false,
  "files.eol": "\n",
  "files.exclude": {
    "**/*.exe": true,
    "**/*.o": true,
    "**/*.olean": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.direnv": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmSync": true,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "nix.enableLanguageServer": true,
  "nix.formatterPath": "nixpkgs-fmt",
  "nix.serverPath": "nil",
  "nix.serverSettings": {
    "nil": {
      "diagnostics": {
        "ignored": [
          "unused_binding",
          "unused_with"
        ]
      },
      "formatting": {
        "command": [
          "nixpkgs-fmt"
        ]
      },
      "nix": {
        "flake": {
          "autoArchive": true,
          "autoEvalInputs": false
        }
      }
    }
  },
  "remote.SSH.defaultExtensions": [
    "Yunduo.color-highlight-css-color-4",
    "MarkosTh09.color-picker",
    "davidbwaters.macos-modern-theme",
    "Comdec.simple-icons",
    "zeno.Mihale",
    "L13RARY.l13-projects",
    "tombonnike.vscode-status-bar-format-toggle",
    "alefragnani.separators",
    "natqe.reload",
    "mhutchie.git-graph",
    "RedCrafter07.red-theme",
    "siyam.Codeify",
    "ms-python.python",
    "jnoortheen.nix-ide",
    "jeff-hykin.better-nix-syntax",
    "aaron-bond.better-comments",
    "dotjoshjohnson.xml",
    "foxundermoon.shell-format",
    "tamasfe.even-better-toml",
    "redhat.vscode-yaml",
    "oderwat.indent-rainbow",
    "DavidAnson.vscode-markdownlint",
    "github.vscode-github-actions",
    "mkhl.direnv",
    "ms-vscode-remote.remote-ssh",
    "PKief.material-product-icons"
  ],
  "remote.SSH.remotePlatform": {
    "10.10.10.187": "linux"
  },
  "remote.SSH.useLocalServer": false,
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.enabled": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 1.8,
  "terminal.integrated.fontFamily": "'Merryweather', 'FiraCode Nerd Font Mono', 'monospace'",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.mouseWheelScrollSensitivity": 2,
  "terminal.integrated.persistentSessionScrollback": 200,
  "terminal.integrated.scrollback": 5000,
  "terminal.integrated.cursorStyleInactive": "block",
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabs.focusMode": "singleClick",
  "update.mode": "none",
  "update.showReleaseNotes": true,
  "window.autoDetectColorScheme": true,
  "window.commandCenter": false,
  "window.density.editorTabHeight": "compact",
  "window.menuBarVisibility": "toggle",
  "window.title": "${rootName}${separator}${profileName}${separator}${activeEditorShort}",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0.3,
  "workbench.colorTheme": "RedCrafter07 Theme",
  "workbench.editor.labelFormat": "short",
  "workbench.enableExperiments": true,
  "workbench.iconTheme": "Mihale",
  "workbench.preferredDarkColorTheme": "Starfall Palenight",
  "workbench.productIconTheme": "macos-modern",
  "workbench.settings.editor": "ui",
  "workbench.settings.enableNaturalLanguageSearch": true,
  "workbench.tree.indent": 4,
  "zenMode.hideLineNumbers": false,
  "_old_theme": {
    "_workbench.colorTheme": "RedCrafter07 Theme",
    "_workbench.preferredDarkColorTheme": "RedCrafter07 Theme",
    "_workbench.colorCustomizations": {
      "_[RedCrafter07 Theme]": {
        "_breadcrumb.background": "#31368b",
        "_editor.background": "#29293e"
      }
    }
  }
}
