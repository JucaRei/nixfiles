{
  "[nix]": {
    "editor.defaultFormatter": "jnoortheen.nix-ide"
  },
  "breadcrumbs.enabled": true,
  "dev.containers.containerEnv": {
    "HOME": "home/node"
  },
  "dev.containers.runArgs": [
    "--userns=keep-id"
  ],
  "docker.composeCommand": "podman-compose",
  "docker.dockerPath": "/nix/store/8cwdz6lf63qk0aryxfv0zc9vbm54g2a8-podman-4.7.2/bin/podman",
  "docker.environment": {
    "DOCKER_HOST": "unix:///run/user/1000/podman/podman.sock"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave.source.fixAll": true,
  "editor.codeActionsOnSave.source.organizeImports": "always",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorSurroundingLines": 2,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.cursorWidth": 2,
  "editor.diffEditor.codeLens": true,
  "editor.diffEditor.experimental.showMoves": true,
  "editor.diffEditor.hideUnchangedRegions.enabled": true,
  "editor.find.autoFindInSelection": "multiline",
  "editor.fontFamily": "'JetbrainsMono Nerd Font SemiBold Italic', 'SauceCodePro Nerd Font Propo Regular Italic', 'Droid Sans Mono'",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontVariations": true,
  "editor.fontWeight": 500,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.indentation": true,
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": true,
  "editor.lineHeight": 1.8,
  "editor.lineNumbers": "on",
  "editor.minimap.autohide": true,
  "editor.minimap.maxColumn": 90,
  "editor.minimap.renderCharecters": false,
  "editor.quickSuggestions.comments": "on",
  "editor.quickSuggestionsdelay": 1,
  "editor.renderWhitespace": "trailing",
  "editor.roundedSelection": true,
  "editor.rulers": [
    80,
    120
  ],
  "editor.scrollbar.horizontal": "auto",
  "editor.scrollbar.vertical": "auto",
  "editor.search.followSymlinks": false,
  "editor.search.searchEditor.doubleClickBehaviour": "goToLocation",
  "editor.search.smartCase": true,
  "editor.search.sortOrder": "default",
  "editor.smoothScrolling": true,
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.tabsize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "indent",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrder": "default",
  "explorer.sortOrderLexicographicOptions": "default",
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.autoGuessEncoding": true,
  "files.autoSave": "afterDelay",
  "files.enableTrash": false,
  "files.eol": "\n",
  "files.exclude": {
    "**/*.exe": true,
    "**/*.o": true,
    "**/*.olean": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.direnv": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmSync": true,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "lldb.suppressUpdateNotifications": true,
  "markdown.extension.print.theme": "dark",
  "markdown.preview.typographer": true,
  "markdownlint.config": {
    "MD024": {
      "siblings_only": true
    },
    "MD028": false,
    "MD040": false,
    "MD041": false
  },
  "nix.enableLanguageServer": true,
  "nix.formatterPath": "/nix/store/g7iw89kgcf1y1cy6ilwhbn38l91mm95a-nixpkgs-fmt-1.3.0/bin/nixpkgs-fmt",
  "nix.serverPath": "/nix/store/id7c0a8ximrcm9p8gxcc0yqsgkmjawg2-nil-2023-08-09/bin/nil",
  "nix.serverSettings": {
    "nil": {
      "diagnostics": {
        "ignored": [
          "unused_binding",
          "unused_with"
        ]
      },
      "formatting": {
        "command": [
          "/nix/store/g7iw89kgcf1y1cy6ilwhbn38l91mm95a-nixpkgs-fmt-1.3.0/bin/nixpkgs-fmt"
        ]
      }
    }
  },
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.enabled": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.fontFamily": "'UbuntuMono Nerd Font Mono Regular', 'monospace'",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.mouseWheelScrollSensitivity": 2,
  "terminal.integrated.persistentSessionScrollback": 200,
  "terminal.integrated.scrollback": 5000,
  "terminal.integrated.shell.linux": "/nix/store/q1c2flcykgr4wwg5a6h450hxbk4ch589-bash-5.2-p15/bin/bash",
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabs.focusMode": "singleClick",
  "update.mode": "none",
  "update.showReleaseNotes": true,
  "window.commandCenter": false,
  "window.density.editorTabHeight": "compact",
  "window.menuBarVisibility": "toggle",
  "window.title": "${rootName}${separator}${profileName}${separator}${activeEditorShort}",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "RedCrafter07 Theme",
  "workbench.editor.labelFormat": "short",
  "workbench.enableExperiments": true,
  "workbench.iconTheme": "bearded-icons",
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "bootstrap-product-icons",
  "workbench.settings.editor": "ui",
  "workbench.settings.enableNaturalLanguageSearch": true,
  "workbench.smoothScrolling": true,
  "workbench.tree.indent": 4,
  "zenMode.hideLineNumbers": false
}
