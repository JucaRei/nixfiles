{
  "[lua]": {
    "editor.defaultFormatter": "yinfei.luahelper"
  },
  "[nix]": {
    "editor.defaultFormatter": "jnoortheen.nix-ide"
  },
  "direnv.restart.automatic": true,
  "direnv.status.showChangesCount": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeActionsOnSave.source.fixAll": true,
  "editor.codeActionsOnSave.source.organizeImports": "always",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorSurroundingLines": 1.2,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.cursorWidth": 1.6,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.diffEditor.codeLens": true,
  "editor.diffEditor.experimental.showMoves": true,
  "editor.diffEditor.hideUnchangedRegions.enabled": true,
  "editor.find.autoFindInSelection": "multiline",
  "editor.fontFamily": "'JetbrainsMono Nerd Font SemiBold Italic', 'SauceCodePro Nerd Font Propo Regular Italic', 'Droid Sans Mono'",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.fontVariations": true,
  "editor.fontWeight": 500,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.indentation": true,
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": true,
  "editor.lineHeight": 1.6,
  "editor.lineNumbers": "on",
  "editor.minimap.autohide": true,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.renderCharecters": false,
  "editor.quickSuggestions.comments": "on",
  "editor.quickSuggestionsdelay": 1,
  "editor.renderWhitespace": "trailing",
  "editor.roundedSelection": true,
  "editor.rulers": [80, 120],
  "editor.scrollbar.horizontal": "auto",
  "editor.scrollbar.vertical": "auto",
  "editor.search.followSymlinks": false,
  "editor.search.searchEditor.doubleClickBehaviour": "goToLocation",
  "editor.search.smartCase": true,
  "editor.search.sortOrder": "default",
  "editor.smoothScrolling": true,
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.tabsize": 1.5,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "indent",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrder": "default",
  "explorer.sortOrderLexicographicOptions": "default",
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.autoGuessEncoding": true,
  "files.autoSave": "afterDelay",
  "files.enableTrash": false,
  "files.eol": "\n",
  "files.exclude": {
    "**/*.exe": true,
    "**/*.o": true,
    "**/*.olean": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.direnv": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmSync": true,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "lldb.suppressUpdateNotifications": true,
  "markdown.extension.print.theme": "dark",
  "markdown.preview.typographer": true,
  "markdownlint.config": {
    "MD024": {
      "siblings_only": true
    },
    "MD028": false,
    "MD040": false,
    "MD041": false
  },
  "nix.enableLanguageServer": true,
  "nix.formatterPath": "nixpkgs-fmt",
  "nix.serverPath": "nil",
  "nix.serverSettings": {
    "nil": {
      "diagnostics": {
        "ignored": ["unused_binding", "unused_with"]
      },
      "formatting": {
        "command": ["nixpkgs-fmt"]
      },
      "nix": {
        "flake": {
          "autoArchive": true,
          "autoEvalInputs": false
        }
      }
    }
  },
  "remote.SSH.defaultExtensions": [
    "ms-python.python",
    "jeff-hykin.better-nix-syntax",
    "Yunduo.color-highlight-css-color-4",
    "davidbwaters.macos-modern-theme",
    "MarkosTh09.color-picker",
    "Comdec.simple-icons",
    "L13RARY.l13-projects",
    "tombonnike.vscode-status-bar-format-toggle",
    "foxundermoon.shell-format",
    "alefragnani.separators",
    "rangav.vscode-thunder-client",
    "natqe.reload",
    "mhutchie.git-graph",
    "adpyke.codesnap",
    "chireia.darker-plus-material-red",
    "beardedbear.beardedicons",
    "nico-castell.linux-desktop-file",
    "RedCrafter07.red-theme",
    "jnoortheen.nix-ide",
    "oderwat.indent-rainbow",
    "formulahendry.code-runner",
    "DavidAnson.vscode-markdownlint",
    "tamasfe.even-better-toml",
    "EditorConfig.EditorConfig",
    "redhat.vscode-yaml",
    "esbenp.prettier-vscode",
    "mkhl.direnv",
    "sumneko.lua",
    "ms-vscode-remote.remote-ssh"
  ],
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.enabled": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 1.8,
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font Mono Ret', 'monospace'",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.mouseWheelScrollSensitivity": 2,
  "terminal.integrated.persistentSessionScrollback": 200,
  "terminal.integrated.scrollback": 5000,
  "terminal.integrated.shell.linux": "/nix/store/dv7spdwndp37znhlq3i6s08azymssyqr-fish-3.7.1/bin/fish",
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabs.focusMode": "singleClick",
  "update.mode": "none",
  "update.showReleaseNotes": true,
  "window.autoDetectColorScheme": true,
  "window.commandCenter": false,
  "window.density.editorTabHeight": "compact",
  "window.menuBarVisibility": "toggle",
  "window.title": "${rootName}${separator}${profileName}${separator}${activeEditorShort}",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0.3,
  "workbench.colorTheme": "RedCrafter07 Theme",
  "workbench.editor.labelFormat": "short",
  "workbench.enableExperiments": true,
  "workbench.iconTheme": "bearded-icons",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "RedCrafter07 Theme",
  "workbench.productIconTheme": "macos-modern",
  "workbench.settings.editor": "ui",
  "workbench.settings.enableNaturalLanguageSearch": true,
  "workbench.smoothScrolling": true,
  "workbench.tree.indent": 4,
  "zenMode.hideLineNumbers": false,
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "remote.SSH.useLocalServer": false
}
